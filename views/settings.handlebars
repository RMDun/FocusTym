<!doctype html>

<html>
<head>
	<title>Settings</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.css" rel="stylesheet">

	<link href="/css/bootstrap-slider.css" rel="stylesheet">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
	
		<link href="css/focusTym.css" rel="stylesheet">
</head>

<body>
	
 	<div class="container">
		<button class="btn" onclick="goBack()"><i class="fa fa-arrow-alt-circle-left"></i> Go Back</button> 
				
	
		<h1>Settings</h1>

		<div class="well">
			<div class="label">Volume</div>
			<input
			    type="text"
			    id="vol"
			    data-provide="slider"
			    data-slider-min="1"
			    data-slider-max="10"
			    data-slider-step="1"
			    data-slider-tooltip="hide"
			>
		</div>
		<form id='snooze' role='form' method='get'>
			<p>Snooze Duration</p>
		   	<div class="form-group">
		   		<select name="snoozeDuration" id="snoozeDuration">
		    		<option value="2"> 2 </option>
		    		<option value="3"> 3 </option>
		    		<option value="4"> 4 </option>
		    		<option value="5"> 5 </option>
		    	</select>
		   		Minutes
		   	</div>
	    		 
		</form>

		<select id="ddlViewBy">
		  <option value="1">test1</option>
		  <option value="2" selected="selected">test2</option>
		  <option value="3">test3</option>
		</select>
		
		<h3>Alarm Options</h3>
		<div class="form-group">
		   	<select name="alarmOptions" id="alarmOptions" onchange="changeAlarm(this)">
		    	<option value="https://www.freespecialeffects.co.uk/soundfx/music/drum_01.wav"> Drum 1 </option>
		    	<option value="https://www.freespecialeffects.co.uk/soundfx/music/drum_09.wav"> Drum 9 </option>
		    	<option value="https://www.freespecialeffects.co.uk/soundfx/music/fanfare7.wav"> Fanfare 7 </option>
		    	<option value="https://www.freespecialeffects.co.uk/soundfx/animals/bird.wav"> Bird </option>
		    	<option value="https://www.freespecialeffects.co.uk/soundfx/animals/frog1.wav"> Frog 1 </option>
	 		</select>
		</div>


	</div>

	


	<script src="/js/jquery-1.11.0.js"></script>
	<script src="/js/bootstrap.js"></script>
	<script src="/js/bootstrap-slider.js"></script>
	<script src="/js/focusTym.js"></script>
	
</body>
</html>