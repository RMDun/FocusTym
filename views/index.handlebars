<!doctype html>

<html>
<head>
	<title>FocusTym</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="css/bootstrap.css" rel="stylesheet">
	<link href="css/bootstrap-theme.css" rel="stylesheet">
	<link href="css/focusTym.css" rel="stylesheet">
</head>

<body>
	<div class="container">
		<h1>FocusTym</h1>
		<hr class=a>
		<div>
			<h2>Group: studs</h2>
			<div class="btn btn-default friends">
		        <button class="btn btn-primary">
		           add
		        </button>
		        New Subject
		   	</div>

		   	{{#each subjects}}
		   	</br>
			<hr>		   	
			<div class="btn btn-default subject">
		        {{name}}
		        <button class="btn btn-primary">
		           edit
		        </button>
		        <button class="btn btn-primary">
		           delet
		        </button>
		    </div>

		    {{/each}}
		</div>

				
	</div>
	<script src="js/jquery-1.11.0.js"></script>
	<script src="js/bootstrap.js"></script>
	<script src="js/focusTym.js"></script>
</body>
</html>